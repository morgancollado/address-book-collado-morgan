<h1> Create Account</h1>

<%=form_tag "/users", method: :post do%>
  <div>
    <%= label_tag :name%>
    <%= text_field_tag :name%>
  </div>
    <div>
    <%= label_tag :email%>
    <%= text_field_tag :email%>
  </div>
    <div>
    <%= label_tag :password%>
    <%= password_field_tag :password%>
  </div>
    
  <%= submit_tag "Sign Up", class: "btn btn-default"%>
<%end%>